      "f2": "eventDescription",
      "f3": "priority",
      "f4": "owner",
      "f5": "eventStartDate",
      "f6": "eventEndDate",
      "f7": "EventDateConfidence",
      "f8": "leads",
      "f9": "eventRing",
      "f10": "eventParty",
      "f11": "eventCategory",
      "f12": "eventCategoryOwner",
      "f13": "eventSubCategory",
      "f14": "eventSubCategoryOwner",
      "f15": "fundingStatus",
      "f16": "hostingType",
      "f17": "city",
      "f18": "countries",
      "f19": "costCenter",
      "f20": "hasSpend",
      "f21": "totalBudget",
      "f22": "splitCostCenter",
      "f23": "costCenterSplit",
      "f24": "partnerInvolved",
      "f25": "partnerName",
      "f26": "responsibilities",
      "f27": "leadFollowUp",
      "f28": "account_segments",
      "f29": "account_segment_type",
      "f30": "buyer_segment_rollups",
      "f31": "industries",
      "f32": "products",
      "f33": "customer_lifecycle",
      "f34": "core_messaging",
      "f35": "tiedToProgram",
      "f36": "adoptAdaptInvent",
      "f37": "statusBasicDetails",
      "f38": "statusExecutionDetails",
      "f39": "readyForActivation",
      "f40": "numberOfInquiries",
      "f41": "venue",
      "f42": "registrationLink",
      "f43": "salesKitLink"
    },
    "sections": [
      {
        "label": "Basic Info",
        "layout": {
          "type": "flex",
          "columns": 2,
          "gap": "16px",
          "flexWrap": "wrap",
          "flexDirection": "column"
        },
        "fields": [
          {
            "id": "f1",
            "name": "eventName",
            "fieldlabel": "Event name",
            "type": "text",
            "validators": ["required"],
            "layout": { "margin_bottom": "0px" }
          },
          {
            "id": "f2",
            "name": "eventDescription",
            "fieldlabel": "Event description",
            "type": "textarea",
            "validators": ["required"],
            "rows": 3,
            "layout": { "margin_bottom": "0px" }
          },
          {
            "name": "priorityOwner",
            "layout": {
              "type": "flex",
              "display": "flex",
              "gap": "16px",
              "flexDirection": "row"
            },
            "subFields": [
              {
                "id": "f3",
                "name": "priority",
                "fieldlabel": "Priority",
                "type": "select",
                "options_key": "f3",
                "validators": ["required"],
                "layout": { "flex": "1" }
              },
              {
                "id": "f4",
                "name": "owner",
                "fieldlabel": "Owner",
                "type": "select",
                "options_key": "f4",
                "validators": ["required"],
                "layout": { "flex": "1" }
              }
            ]
          },
          {
            "name": "eventDates",
            "layout": {
              "type": "flex",
              "display": "flex",
              "gap": "16px",
              "flexDirection": "row"
            },
            "subFields": [
              {
                "id": "f5",
                "name": "eventStartDate",
                "fieldlabel": "Event start date",
                "type": "date",
                "validators": ["required"],
                "layout": { "flex": "1" }
              },
              {
                "id": "f6",
                "name": "eventEndDate",
                "fieldlabel": "Event end date",
                "type": "date",
                "validators": ["required"],
                "layout": { "flex": "1" }
              }
            ]
          },
          {
            "name": "eventDateConfidenceAndLeads",
            "layout": {
              "type": "flex",
              "display": "flex",
              "gap": "16px",
              "flexDirection": "row"
            },
            "subFields": [
              {
                "id": "f7",
                "name": "EventDateConfidence",
                "fieldlabel": "Event date confidence*",
                "type": "select",
                "options_key": "f7",
                "validators": ["required"],
                "layout": { "flex": "1" }
              },
              {
                "id": "f8",
                "name": "leads",
                "fieldlabel": "Leads Expected?*",
                "type": "select",
                "options_key": "f8",
                "validators": ["required"],
                "layout": { "flex": "1" }
              }
            ]
          },
          {
            "id": "f40",
            "name": "numberOfInquiries",
            "fieldlabel": "# of Inquiries",
            "type": "number",
            "visible_when": { "field": "f8", "equals": ["Yes"] }
          },
          {
            "name": "eventDetail",
            "label": "Event categorization",
            "layout": { "type": "grid", "columns": 2, "gap": "16px" },
            "gridFields": [
              {
                "id": "f9",
                "name": "eventRing",
                "fieldlabel": "Event ring*",
                "type": "select",
                "options_key": "f9",
                "validators": ["required"]
              },
              {
                "id": "f10",
                "name": "eventParty",
                "fieldlabel": "Event Party*",
                "type": "select",
                "options_key": "f10",
                "validators": ["required"]
              },
              {
                "id": "f11",
                "name": "eventCategory",
                "fieldlabel": "Event category*",
                "type": "select",
                "options": [],
                "dependsOn": ["f9", "f10"],
                "options_key": "f11"
              },
              {
                "id": "f12",
                "name": "eventCategoryOwner",
                "fieldlabel": "Event category owner*",
                "type": "select",
                "options_key": "f4",
                "validators": ["required"]
              },
              {
                "id": "f13",
                "name": "eventSubCategory",
                "fieldlabel": "Event sub category",
                "type": "select",
                "options": [],
                "dependsOn": ["f9", "f10", "f11"],
                "options_key": "f11"
              },
              {
                "id": "f14",
                "name": "eventSubCategoryOwner",
                "fieldlabel": "Event sub category owner*",
                "type": "select",
                "options_key": "f4",
                "validators": ["required"]
              }
            ]
          }
        ]
      },
      {
        "label": "Logistics",
        "layout": { "type": "flex", "flexDirection": "column", "gap": "16px" },
        "fields": [
          {
            "id": "f15",
            "name": "fundingStatus",
            "fieldlabel": "Funding status*",
            "type": "select",
            "options_key": "f15",
            "validators": ["required"]
          },
          {
            "id": "f16",
            "name": "hostingType",
            "fieldlabel": "Hosting type*",
            "type": "select",
            "options_key": "f16",
            "validators": ["required"]
          },
          {
            "id": "f17",
            "name": "city",
            "fieldlabel": "City",
            "type": "select",
            "options_key": "f17",
            "visible_when": { "field": "f16", "equals": ["Physical Event", "Hybrid Event"] }
          },
          {
            "id": "f18",
            "name": "countries",
            "fieldlabel": "Country",
            "type": "select",
            "options_key": "f18",
            "visible_when": { "field": "f16", "equals": ["Digital Event"] }
          }
        ]
      },
      {
        "label": "Financial Details",
        "layout": { "type": "flex", "flexDirection": "column", "gap": "16px" },
        "fields": [
          {
            "id": "f19",
            "name": "costCenter",
            "fieldlabel": "Cloud marketing cost center*",
            "type": "select",
            "options_key": "f19",
            "validators": ["required"]
          },
          {
            "id": "f20",
            "name": "hasSpend",
            "fieldlabel": "Does this tactic have spend?*",
            "type": "select",
            "options_key": "f20",
            "validators": ["required"]
          },
          {
            "id": "f21",
            "name": "totalBudget",
            "fieldlabel": "Total budget*",
            "type": "number",
            "validators": ["required"],
            "visible_when": { "field": "f20", "equals": ["Yes"] }
          },
          {
            "id": "f22",
            "name": "splitCostCenter",
            "fieldlabel": "Split budget between cost centers?",
            "type": "select",
            "options_key": "f22",
            "validators": ["required"],
            "visible_when": { "field": "f20", "equals": ["Yes"] }
          },
          {
            "id": "f23",
            "name": "costCenterSplit",
            "fieldlabel": "Cost center split",
            "type": "array",
            "visible_when": { "field": "f22", "equals": ["Yes"] },
            "itemTemplate": {
              "layout": { "type": "flex", "gap": "16px" },
              "subFields": [
                {
                  "name": "costCenter",
                  "type": "select",
                  "options_key": "f19",
                  "validators": ["required"],
                  "layout": { "flex": "1" }
                },
                {
                  "name": "splitPercentage",
                  "type": "number",
                  "validators": ["required", "percentage"],
                  "layout": { "flex": "1" }
                }
              ]
            }
          }
        ]
      },
      {
        "label": "Extras",
        "layout": { "type": "flex", "flexDirection": "column", "gap": "16px" },
        "fields": [
          { "id": "f41", "name": "venue", "fieldlabel": "Venue", "type": "text" },
          {
            "id": "f42",
            "name": "registrationLink",
            "fieldlabel": "Registration Link",
            "type": "text"
          },
          {
            "id": "f43",
            "name": "salesKitLink",
            "fieldlabel": "Sales Kit Link",
            "type": "text"
          }
        ]
      },
      {
        "label": "Partner Details",
        "layout": { "type": "flex", "flexDirection": "column", "gap": "16px" },
        "fields": [
          {
            "id": "f24",
            "name": "partnerInvolved",
            "fieldlabel": "Is a partner involved?*",
            "type": "select",
            "options_key": "f24",
            "validators": ["required"]
          },
          {
            "id": "f25",
            "name": "partnerName",
            "fieldlabel": "Partner name*",
            "type": "text",
            "validators": ["required"],
            "visible_when": { "field": "f24", "not_equals": ["No Partner Involvement"] }
          },
          {
            "id": "f26",
            "name": "responsibilities",
            "fieldlabel": "Partner responsibilities",
            "type": "select",
            "options_key": "f26",
            "visible_when": { "field": "f24", "not_equals": ["No Partner Involvement"] }
          },
          {
            "id": "f27",
            "name": "leadFollowUp",
            "fieldlabel": "Who will follow up with the leads?*",
            "type": "select",
            "options_key": "f27",
            "validators": ["required"],
            "visible_when": { "field": "f24", "not_equals": ["No Partner Involvement"] }
          }
        ]
      },
      {
        "label": "Alignments",
        "layout": { "type": "grid", "columns": 2, "gap": "16px" },
        "gridFields": [
          {
            "id": "f28",
            "name": "account_segments",
            "fieldlabel": "Account Segments",
            "type": "select",
            "options_key": "f28"
          },
          {
            "id": "f29",
            "name": "account_segment_type",
            "fieldlabel": "Account Segment Type",
            "type": "select",
            "options_key": "f29"
          },
          {
            "id": "f30",
            "name": "buyer_segment_rollups",
            "fieldlabel": "Buyer Segment Rollups",
            "type": "select",
            "options_key": "f30"
          },
          {
            "id": "f31",
            "name": "industries",
            "fieldlabel": "Industries",
            "type": "select",
            "options_key": "f31"
          },
          {
            "id": "f32",
            "name": "products",
            "fieldlabel": "Products",
            "type": "select",
            "options_key": "f32"
          },
          {
            "id": "f33",
            "name": "customer_lifecycle",
            "fieldlabel": "Customer Lifecycle",
            "type": "select",
            "options_key": "f33"
          },
          {
            "id": "f34",
            "name": "core_messaging",
            "fieldlabel": "Core Messaging",
            "type": "select",
            "options_key": "f34"
          }
        ]
      },
      {
        "label": "Campaign Program",
        "layout": { "type": "flex", "flexDirection": "column", "gap": "16px" },
        "fields": [
          {
            "id": "f35",
            "name": "tiedToProgram",
            "fieldlabel": "Is this tactic tied to a global campaign program?*",
            "type": "select",
            "options_key": "f35",
            "validators": ["required"]
          },
          {
            "id": "f36",
            "name": "adoptAdaptInvent",
            "fieldlabel": "Adopt / Adapt / Invent*",
            "type": "select",
            "options_key": "f36",
            "dependsOn": "f35",
            "validators": ["required"]
          }
        ]
      },
      {
        "label": "Review & Submit",
        "fields": [
          {
            "id": "f37",
            "name": "statusBasicDetails",
            "fieldlabel": "Basic details status",
            "type": "boolean"
          },
          {
            "id": "f38",
            "name": "statusExecutionDetails",
            "fieldlabel": "Execution details status",
            "type": "boolean"
          },
          {
            "id": "f39",
            "name": "readyForActivation",
            "fieldlabel": "Ready for activation",
            "type": "boolean"
          }
        ]
      }
    ]
  },
